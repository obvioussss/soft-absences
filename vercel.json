{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/style.css",
      "dest": "/api/index.py"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  }
}